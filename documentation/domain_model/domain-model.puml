    }

    class NutricionalInfo <<value object>> {
    }

    class Money <<value object>> {
    }

    class Designation <<value object>> <<id>> {
    }

    Dish --> "1" Designation
    Dish --> "1" NutricionalInfo
    Dish --> "1" Money
    Dish --> "1" Description
    Dish --> "1" DishType
